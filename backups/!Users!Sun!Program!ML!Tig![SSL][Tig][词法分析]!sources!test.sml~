CM.make "sources.cm";(*编译*)
fun test (dir, outdir) =
         let
			 val t0 = "test0.tig"
			 val t1 = "test1.tig"
     		 val t2 = "test2.tig"
		     val t3 = "test3.tig"
		     val t4 = "test4.tig"
         in
           (
             print(dir ^ "/" ^ t0 ^ "\n");
             Parse.parse(dir ^ "/" ^ t0,TextIO.openOut (outdir ^ "/" ^ t0));
             print(dir ^ "/" ^ t1 ^ "\n");
             Parse.parse(dir ^ "/" ^ t1,TextIO.openOut (outdir ^ "/" ^ t1));
             print(dir ^ "/" ^ t2 ^ "\n");
             Parse.parse(dir ^ "/" ^ t2,TextIO.openOut (outdir ^ "/" ^ t2));
             print(dir ^ "/" ^ t3 ^ "\n");
             Parse.parse(dir ^ "/" ^ t3,TextIO.openOut (outdir ^ "/" ^ t3));
             print(dir ^ "/" ^ t4 ^ "\n");
             Parse.parse(dir ^ "/" ^ t4,TextIO.openOut (outdir ^ "/" ^ t4))
           )
	   end
	   ;
	   
	   test("test", "out");
              
              
