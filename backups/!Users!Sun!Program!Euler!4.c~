#include "stdio.h"
int palindrome(int a){
  int b=0,temp=a;
  while(a>0)
    {
      b*=10;
      b+=a%10;
      a/=10;
    }
  if(b==temp)
    return 1;
  else
    return 0;
}

int main(int argc, char *argv[])
{
  int a;
  int b;
  for(a=999; a>100; a--){
    for(b=999; b>100; b--){
      if(palindrome(a*b) == 1){
        printf ("%d\n", a*b);
        return 0;
      }
    }
  }
  
  return 0;
}
