#include "stdio.h"
#include <math.h>
int prime(int a){
  int n;
  for (n = 2; n < a; n++)
    {
      if(a%n ==0)
        return 0;
    }
  return 1;
}

int main(int argc, char *argv[])
{
  int i;
  int sum = 0;
  for (i = 2; i < 2000000; i++)
    {
      if(prime(i) == 1)
        sum += i;
    }

  printf ("%d\n", sum);
  return 0;
}
